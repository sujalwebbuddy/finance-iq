{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "IS_VERCEL": "true"
    },
    "functions": {
      "server.js": {
        "maxDuration": 30
      }
    }
  }